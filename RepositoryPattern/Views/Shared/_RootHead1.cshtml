@using ClientDependency.Core
@using ClientDependency.Core.Mvc
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Bootstrap1/css")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @MvcHtmlString.Create(Html.RenderCssHere(new List<IClientDependencyPath>
												{
													new BasicPath("Content", "~/Content")
												}).ToString())
</head>
<body>
    @RenderBody()
   
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundle/bootstrap")
    @MvcHtmlString.Create(Html.RenderJsHere(new List<IClientDependencyPath>
	    {
		    new BasicPath("Custom", "~/Scripts/js")
	    }).ToString())
    
  
</body>
</html>
