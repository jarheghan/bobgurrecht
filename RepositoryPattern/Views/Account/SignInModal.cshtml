
<form role="form">
  <div class="form-group">
    @Html.Label("Email Address")
    @Html.TextBox("username",null,new{@class="form-control",placeholder="Email Address"})
   
  </div>
  <div class="form-group">
    @Html.Label("Password")
    @Html.Password("password",null,new{@class="form-control",placeholder="Password"})
  </div>
</form>