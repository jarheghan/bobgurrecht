
    <div class="panel panel-default">
        <div class="panel-heading">Login Information </div>
        <div class="panel-body">
                <div class="form-group">
                    @Html.Label("Username", new { @class = "sr-only" })
                    @Html.TextBox("username", null, new { @class = "form-control", placeholder = "Email" })
                </div>
                <div class="form-group">
                    @Html.Label("Password", new { @class = "sr-only" })
                    @Html.Password("password", null, new { @class = "form-control", placeholder = "Password" })
                </div>
                <div class="form-group">
                    @Html.Label("Comfirm Password", new { @class = "sr-only" })
                    @Html.Password("compassword", null, new { @class = "form-control", placeholder = "Re-Enter Password" })
                </div>
        </div>
    </div>
